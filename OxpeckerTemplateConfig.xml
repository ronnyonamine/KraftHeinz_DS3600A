            <ReleaseType>P</ReleaseType>
            <MajorVersion>00</MajorVersion>
            <MinorVersion>001</MinorVersion>
            <ReleaseLabel>N</ReleaseLabel>
            <Version>99</Version>
            <SuffixLabel>M</SuffixLabel>
            <SuffixVersion>0</SuffixVersion>
        </VersionDef>
    </Versions>
</OxPackConfig>