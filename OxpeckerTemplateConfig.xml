        <PathDef>
            <Name>productcodeHexFolderPath</Name>
            <Value>"Release\hex"</Value>
        </PathDef>
        <PathDef>
            <Name>combinedHexFolderPath</Name>
            <Value>"Release\hex"</Value>
        </PathDef>
        <PathDef>
            <Name>projectBinFile</Name>
            <Value>"Release\bin\project.bin"</Value>
        </PathDef>
    </Paths>
    <Versions>
        <VersionDef>
            <!-- Version for productCode                                     -->
            <!-- Sample version is PAAFIS00-001-N43M0.hex                    -->
